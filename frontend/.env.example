# Frontend Environment Configuration
# Copy this file to .env and update with your specific values

# API URL - Backend API endpoint
VITE_API_URL=http://localhost:5000/api

# Environment - (development, production, test)
VITE_APP_ENV=development

# Feature flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DOCUMENT_ANALYSIS=false

# Optional - Mock API (for development without a backend)
# VITE_USE_MOCK_API=true

# Deployment URLs
VITE_PRODUCTION_URL=https://leadverifypro.vercel.app
VITE_DEVELOPMENT_URL=http://localhost:3000 